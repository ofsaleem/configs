set nocompatible
filetype off
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'gmarik/Vundle.vim'
Plugin 'flazz/vim-colorschemes'

"set term=screen-256color
syntax enable
set background=dark
colorscheme solarized
set tabstop=4
set noexpandtab
set autoindent
set shiftwidth=2
set smarttab
set formatoptions=croqla
